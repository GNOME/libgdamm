(define-signal row_inserted
  (of-object "GdaDataModel")
  (return-type "void")
  (parameters
    '("gint" "row")
  )
  (when "last")
)

(define-signal row_updated
  (of-object "GdaDataModel")
  (return-type "void")
  (parameters
    '("gint" "row")
  )
  (when "last")
)

(define-signal row_removed
  (of-object "GdaDataModel")
  (return-type "void")
